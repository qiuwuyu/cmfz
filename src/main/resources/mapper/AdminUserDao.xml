		      <!-- com.baizhi.ssm.dao.UserDao.getUser -->
<mapper namespace="com.baizhi.dao.AdminUserDao">
	<!-- 查询账户 -->
	<select id="getAdminUser" resultType="com.baizhi.entity.AdminUser">
		select * from admin_User where username = #{username} and password = #{password}
	</select>
</mapper>
	
